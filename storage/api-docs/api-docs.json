        "/api/v1/posts/keyword/{keyword}": {
            "get": {
                "tags": [
                    "Post"
                ],
                "summary": "Displays posts containing a specific keyword",
                "operationId": "2c7eba5a6e115574bad5d1f307187c7e",
                "parameters": [
                    {
                        "name": "keyword",
                        "in": "path",
                        "description": "Keyword to search in posts",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "alias"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Text found"
                                        },
                                        "quantity of posts with text": {
                                            "type": "integer",
                                            "example": 5
                                        },
                                        "status": {
                                            "type": "integer",
                                            "example": 200
                                        },
                                        "Post(s) with found content(s)": {
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "userId": {
                                                        "type": "integer",
                                                        "example": 1
                                                    },
                                                    "id": {
                                                        "type": "integer",
                                                        "example": 5
                                                    },
                                                    "title": {
                                                        "type": "string",
                                                        "example": "nesciunt quas odio"
                                                    },
                                                    "body": {
                                                        "type": "string",
                                                        "example": "repudiandae veniam quaerat sunt sed alias aut fugiat sit autem sed est voluptatem omnis possimus esse voluptatibus quis est aut tenetur dolor neque"
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "No posts found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Provided text was not found"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
